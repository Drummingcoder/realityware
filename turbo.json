{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"envMode": "loose",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "target/release/**"]
		},
		"backend#build": {
			"inputs": ["src/**/*.rs", "Cargo.toml", "Cargo.lock", "migrations/**"],
			"outputs": ["target/release/**"]
		},
		"web#build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"cache": false
		},
		"postinstall": {
			"cache": false
		}
	}
}
